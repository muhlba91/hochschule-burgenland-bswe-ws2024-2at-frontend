{
  "name": "@muhlba91/hochschule-burgenland-bswe-ws2024-2at-frontend",
  "version": "0.0.0",
  "description": "Hochschule Burgenland - BSWE - WS2024 - 2nd Attempt - Weather App - Frontend - Reference",
  "type": "module",
  "repository": "https://github.com/muhlba91/hochschule-burgenland-bswe-ws2024-2at-frontend",
  "license": "MIT",
  "keywords": [
    "fh-burgenland",
    "reference-project"
  ],
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "fix": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "cyclonedx": "cyclonedx-yarn --production --output-file .quasar/sbom.json -v",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:5173 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:5173 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "packageManager": "yarn@4.12.0",
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "leaflet": "^1.9.4",
    "quasar": "2.18.4",
    "vue": "^3.5.26",
    "vue-router": "^5.0.0"
  },
  "devDependencies": {
    "@cyclonedx/yarn-plugin-cyclonedx": "^3.2.1",
    "@eslint/js": "^10.0.0",
    "@quasar/app-vite": "2.3.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.2.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "cypress": "^15.8.1",
    "eslint": "^10.0.0",
    "eslint-plugin-cypress": "^6.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.12.0",
    "vitest": "^4.0.18",
    "vue-router-mock": "^2.0.0",
    "vue-tsc": "^3.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
